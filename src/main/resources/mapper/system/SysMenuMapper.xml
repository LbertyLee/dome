<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lh.dome.system.mapper.SysMenuMapper">


    <select id="getPermissionList" resultType="java.lang.String">
        SELECT m.perms   FROM sys_menu as m LEFT JOIN sys_role_menu as rm ON as.menu_id=m.menu_id
    </select>
</mapper>
